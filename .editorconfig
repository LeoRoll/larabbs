# coding style between different sditors and IDEs
# editorconfig.org

root = true

[*]

# Change these setting to your own preference
indent_style = space
indent_size = 4

# We recommend you to keep these unchange
end_of_file = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = false

[*.{js,html,blade.php,css,scss}]
indent_style = space
indent_size = 4

[*.md]
trim_trailing_whitespace = false  